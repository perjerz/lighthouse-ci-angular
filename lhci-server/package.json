  },
  "dependencies": {
    "@lhci/cli": "0.3.4",
    "@lhci/server": "0.3.4",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sqlite3": "^4.0.6"
  }
}
